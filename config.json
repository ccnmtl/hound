{
    "Alerts": [
        {
            "Name": "Capsim Smoketests Failed",
            "Metric": "app.smoketest.capsim.failed",
            "Threshold": 0.5555555555555555555555555555,
            "Direction": "above",
            "EmailTo": "anders2@columbia.edu"
        },
        {
            "Name": "Capsim Smoketests Passed",
            "Metric": "app.smoketest.capsim.passed",
            "Threshold": 5.5,
            "Direction": "below"
        },
        {
            "Name": "Capsim Smoketests Passed Notice",
            "Type": "Notice",
            "Metric": "app.smoketest.capsim.passed",
            "Threshold": 5.5,
            "Direction": "below"
        },
        {
            "Name": "MVSim Smoketests Failed",
            "Metric": "app.smoketest.mvsim.failed",
            "Threshold": 0.5,
            "Direction": "above"
        },
        {
            "Name": "cement highstate failed",
            "Type": "Notice",
            "Metric": "summarize(server.cement.highstate.failed,'1h','min')",
            "Threshold": 0.5,
            "Direction": "above"
        }
    ]
}
